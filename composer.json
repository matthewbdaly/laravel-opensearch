{
    "name": "matthewbdaly/laravel-opensearch",
    "description": "Opensearch implementation for Laravel",
    "type": "library",
    "keywords": ["laravel","lumen", "search", "opensearch"],
    "require": {},
    "require-dev": {
        "orchestra/testbench": "3.5",
        "phpunit/phpunit": "^6.3",
        "mockery/mockery": "~1.0",
        "squizlabs/php_codesniffer": "^3.1",
        "psy/psysh": "0.8.*",
        "matthewbdaly/artisan-standalone": "0.0.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Matthew Daly",
            "email": "matthewbdaly@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Matthewbdaly\\LaravelOpensearch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Matthewbdaly\\LaravelOpensearch\\Providers\\ServiceProvider"
            ]
        }
    }
}
